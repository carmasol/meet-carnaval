<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
		<link rel="stylesheet" href="{{ asset('css/main.css') }}"/>
	</head>
	<div class="content">


		<body>
			<div class="overlay">

				{{ include('navbar.html.twig') }}

				<main class="container pt-5"> {% block body %}{% endblock %}
					</main>
					<br>
					<br>
					<br>
					<br>
					<br>

					<footer>
						<div class="footer">
							<div class='mail'>
								<a href="{{ path('app_contact') }}">
                                <img src="{{ asset('/images/mail.png') }}" alt="contact">
							</div>
                            <a>

							<div class='facebook'>
								<a href="https://www.facebook.com/diti.wayd/">
									<img src="{{ asset('/images/facebook.png') }}" alt="facebook">
								</div>
							</a>

							<div class='instagram'>
								<a href="https://www.instagram.com/diti.web/"><img src="{{ asset('/images/instagram.png') }}" alt="instagram">
								</div>
							</a>

							<div class='pinterest'>
								<a href="https://www.pinterest.fr/carmasoldietric/"><img src="{{ asset('/images/pinterest.png') }}" alt="pinterest">
								</div>
							</a>
						</div>
					</footer>

					<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
				</div>
			</body>
		</div>
	</html>
</body></html></html>
